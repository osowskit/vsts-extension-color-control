{
    "manifestVersion": 1,
    "id": "microsoft-github-app-extension",
    "version": "1.0.8",
    "name": "GitHub App Extension",
    "description": "Create GitHub branches from VSTS.",
    "publisher": "osowskit",
    "icons": {
        "default": "img/logo.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "tags": [
        "Work Item",
        "Work Item control",
        "Pick list",
        "Color coded"
    ],
    "content": {
        "details": {
            "path": "details.md"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/osowskit/microsoft-github-app-extension"
    },
    "links": {
        "learn": {
            "uri": "https://github.com/osowskit/microsoft-github-app-extension/blob/master/README.md"
        },
        "support": {
            "uri": "https://github.com/osowskit/microsoft-github-app-extension"
        },
        "repository": {
            "uri": "https://github.com/osowskit/microsoft-github-app-extension"
        },
        "issues": {
            "uri": "https://github.com/osowskit/microsoft-github-app-extension/issues"
        }
    },
    "files": [
        {
            "path": "img",
            "addressable": true
        },
        {
            "path": "index.html",
            "addressable": true
        }
    ],
    "categories": [
        "Plan and track"
    ],
    "contributions": [
        {
            "id": "color-control-contribution",
            "type": "ms.vss-work-web.work-item-form-control",
            "description": "Create GitHub branch VSTS",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "GitHub Branch Selector",
                "uri": "https://microsoft-github-app-extension.herokuapp.com/main_entry",
                "height": 90
            }
        }
    ]
}
